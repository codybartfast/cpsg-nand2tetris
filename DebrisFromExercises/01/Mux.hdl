/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);
    And(a=a, b=notSel, out=aSel);
    And(a=sel, b=b, out=bSel);
    Or(a=aSel, b=bSel, out=out);
}
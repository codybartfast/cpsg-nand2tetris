/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nand);
    Nand(a=a, b=nand, out=nandA);
    Nand(a=nand, b=b, out=nandB);
    Nand(a=nandA, b=nandB, out=out);
}
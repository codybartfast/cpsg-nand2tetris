/**
 * The Main class initializes a new Match Dance game and starts it.
 */
class Main {

    /** Initializes a new game and starts it. */    
    function void main() {
        var MatchGame game;

        let game = MatchGame.new();
        do game.run();
				do game.dispose();

        return;
    }
}
